VAR arr0, arr1, arr2, tmp;

BEGIN
    arr0 := 4; arr1 := 3; arr2 := 9;

    IF arr0 > arr1 THEN
    BEGIN
        tmp := arr0;
        arr0 := arr1;
        arr1 := tmp;
    END;

    IF arr1 > arr2 THEN
    BEGIN
        tmp := arr1;
        arr1 := arr2;
        arr2 := tmp;
    END;

    IF arr0 > arr1 THEN
    BEGIN
        tmp := arr0;
        arr0 := arr1;
        arr1 := tmp;
    END;

    WRITE arr0;
    WRITE arr1;
    WRITE arr2;
END.